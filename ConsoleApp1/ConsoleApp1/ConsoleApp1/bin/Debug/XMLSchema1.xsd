<?xml version="1.0" encoding="utf-8"?>
<xs:schema elementFormDefault="qualified"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
>
  <xs:element name="Workers">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Developer">
          <xs:complexType>
            <xs:sequence>
              <xs:element name ="id" type ="xs:int" />
              <xs:element name ="FirstName" type ="xs:string" />
              <xs:element name ="LastName" type ="xs:string" />              
              <xs:element name ="Sex" type ="xs:int" />
              <xs:element name ="Appointment" type ="xs:string" />
              <xs:element name ="Date" type ="xs:string" />
              <xs:element name ="Salary" type ="xs:int" />
              <xs:element name ="DeveloperLanguage" type ="xs:string" />
              <xs:element name ="DeveloperLang" type ="xs:string" />
              <xs:element name ="Experience" type ="xs:int" />
              <xs:element name ="Level" type ="xs:string" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="OfficeWorker">
          <xs:complexType>
            <xs:sequence>
              <xs:element name ="id" type ="xs:int" />
              <xs:element name ="FirstName" type ="xs:string" />
              <xs:element name ="LastName" type ="xs:string" />              
              <xs:element name ="Sex" type ="xs:int" />
              <xs:element name ="Appointment" type ="xs:string" />
              <xs:element name ="Date" type ="xs:string" />
              <xs:element name ="Salary" type ="xs:int" />
              <xs:element name ="YearsInService" type="xs:string" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>  
</xs:schema>
